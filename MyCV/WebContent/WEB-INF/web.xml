<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	id="WebApp_ID" version="2.5">
	<display-name>MyCV</display-name>
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
	<servlet>
		<description></description>
		<display-name>AuthenticationServlet</display-name>
		<servlet-name>AuthenticationServlet</servlet-name>
		<servlet-class>com.dev.myproject.AuthenticationServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AuthenticationServlet</servlet-name>
		<url-pattern>/authentication</url-pattern>
	</servlet-mapping>
	<servlet>
		<description></description>
		<display-name>LoginServlet</display-name>
		<servlet-name>LoginServlet</servlet-name>
		<servlet-class>com.dev.myproject.LoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LoginServlet</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>LoginPage</servlet-name>
		<jsp-file>/login.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>LoginPage</servlet-name>
		<url-pattern>/login-page</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>CVPage</servlet-name>
		<jsp-file>/cv-page.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>CVPage</servlet-name>
		<url-pattern>/chauquocminh-curriculum-vitae</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>ErrorPage</servlet-name>
		<jsp-file>/error-page.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>ErrorPage</servlet-name>
		<url-pattern>/error</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>UserList</servlet-name>
		<jsp-file>/user-list.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>UserList</servlet-name>
		<url-pattern>/user-list</url-pattern>
	</servlet-mapping>
	<servlet>
		<description></description>
		<display-name>EmailServlet</display-name>
		<servlet-name>EmailServlet</servlet-name>
		<servlet-class>com.dev.myproject.EmailServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>EmailServlet</servlet-name>
		<url-pattern>/send-email</url-pattern>
	</servlet-mapping>
	<servlet>
		<description></description>
		<display-name>ConfirmRequestServlet</display-name>
		<servlet-name>ConfirmRequestServlet</servlet-name>
		<servlet-class>com.dev.myproject.ConfirmRequestServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ConfirmRequestServlet</servlet-name>
		<url-pattern>/confirm-request</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>ConfirmationPage</servlet-name>
		<jsp-file>/confirmation-page.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>ConfirmationPage</servlet-name>
		<url-pattern>/confirmation-page</url-pattern>
	</servlet-mapping>
</web-app>